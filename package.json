{
  "name": "@furvester/upstream-sync",
  "version": "1.1.1",
  "description": "Sync from upstream via RabbitMQ",
  "type": "module",
  "author": "Ben Scholzen 'DASPRiD'",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Furvester/upstream-sync.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
